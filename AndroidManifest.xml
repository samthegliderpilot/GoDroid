<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:a="http://schemas.android.com/apk/res/android"
	package="de.agrothe.go"
	a:versionCode="35"
	a:versionName="1.3.8"
	a:installLocation="auto">
	<uses-sdk a:minSdkVersion="4" a:targetSdkVersion="8"/>
	<uses-permission a:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
	<supports-screens a:largeScreens="true"
		a:normalScreens="true"
		a:smallScreens="true"
		a:anyDensity="true"/>
	<!--
	a:debuggable="false"
	-->
	<application a:label="@string/app_name"
		a:icon="@drawable/activity_icon"
		a:description="@string/app_description"
		a:theme="@style/Theme.NoBackground">
		<activity a:name=".MainActivity"
			a:launchMode="singleTask"
			a:screenOrientation="portrait">
			<intent-filter>
				<action a:name="android.intent.action.MAIN"/>
				<category a:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
			<intent-filter>
				<action a:name="android.intent.action.VIEW"/>
				<category a:name="android.intent.category.DEFAULT"/>
				<category a:name="android.intent.category.BROWSABLE"/>
				<data a:scheme="file" a:mimeType="application/x-go-sgf"/>
				<data a:scheme="content" a:mimeType="application/x-go-sgf"/>
			</intent-filter>
		</activity>
	</application>
</manifest>
